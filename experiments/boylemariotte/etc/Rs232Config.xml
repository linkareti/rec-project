<?xml version="1.0" encoding="iso-8859-1" ?>
<hardware id="ELAB_BOYLE_MARIOTTE_DSPIC_V1.0" num_channels="2" minfrequency="0.1Hz" maxfrequency="1000Hz">
  <rs232 baud="115200" stopbits="1" paritybits="0" numbits="8" ports_restrict="/dev/ttyS0">
	<parameters>
	    <!-- vol_init -->
	    <parameter output="#000" input="#000" maxvalue="1000" minvalue="100" order="0">
	    </parameter>
	    <!-- vol_final -->
	    <parameter output="#000" input="#000" maxvalue="1000" minvalue="100" order="1">
	    </parameter>
	    <!--n_samples -->
	    <parameter output="#0" input="#0" maxvalue="45" minvalue="2" order="2">
	    </parameter>
	    <!-- tb_samples -->
	    <parameter output="#000" input="#000" maxvalue="2000" minvalue="700" order="3">
	    </parameter>
	</parameters>
	<channels>
	    <!-- volume -->
	    <channel format="##00" order="0">
	    </channel>
	    <!-- pressure -->
	    <channel format="#00" order="1">
	    </channel>
	</channels>
	<timeout>
	    <default_timeout time="45000" />
	    <id time="5000" />
	    <cfg time="10000" />
	    <cur time="5000" />
	    <str time="5000" />
	    <dat_bin time="210000" />
	    <no_data time="10000" />
	    <end time="210000" />
	    <stp time="10000" />
	    <rst time="60000" />
	    <hardware_died time="3600000" />
	    <listen_to_port time="10000" />
	</timeout>
	<errors>
	    <error code="0" key="OPS" message="Something wrong happened !" />
	    <error code="1" key="SENSOR" message="Sensor has failed." />
	    <error code="2" key="OUT" message="Value out of range..." />
	</errors>
  </rs232>
</hardware>
