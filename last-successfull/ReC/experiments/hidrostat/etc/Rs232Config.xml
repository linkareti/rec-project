<?xml version="1.0" encoding="iso-8859-1" ?>
<hardware id="ELAB_HIDROSTAT_DSPIC_V1.0" num_channels="5"
	minfrequency="0.2Hz" maxfrequency="0.5Hz">
	<rs232 baud="115200" stopbits="1" paritybits="0" numbits="8"
		ports_restrict="/dev/ttyUSB0">
		<parameters>
			<!-- protocolo -->
			<parameter output="0" input="0" maxvalue="1" minvalue="1"
				order="0">
			</parameter>
			<!-- profundidade inicial -->
			<parameter output="###0" input="###0" maxvalue="1930"
				minvalue="0" order="1">
			</parameter>
			<!-- profundidade final -->
			<parameter output="###0" input="###0" maxvalue="1930"
				minvalue="0" order="2">
			</parameter>
			<!-- numero de pontos -->
			<parameter output="##0" input="##0" maxvalue="200"
				minvalue="2" order="3">
			</parameter>
		</parameters>
		<channels>
			<!-- profundidade -->
			<channel format="0.000" order="0">
			</channel>
			<!-- pressao 1 -->
			<channel format="#00.000" order="1">
			</channel>
			<!-- pressao 2 -->
			<channel format="#00.000" order="2">
			</channel>
			<!-- pressao 3 -->
			<channel format="#00.000" order="3">
			</channel>
			<!-- pressao 4 -->
			<channel format="#00.000" order="4">
			</channel>
		</channels>
		<timeout>
			<default_timeout time="45000" />
			<id time="20000" />
			<cfg time="300000" />
			<cur time="5000" />
			<str time="10000" />
			<dat_bin time="120000" />
			<no_data time="120000" />
			<end time="600000" />
			<stp time="120000" />
			<rst time="120000" />
			<hardware_died time="3600000" />
			<listen_to_port time="10000" />
		</timeout>
		<errors>
			<error code="0" key="OPS" message="Something wrong happened !" />
		</errors>
	</rs232>
</hardware>
