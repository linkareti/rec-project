uency type="SamplingIntervalType" multiplier="none">15</Frequency>	<sampling-scale>		<min-samples>10</min-samples>		<max-samples>2500</max-samples>		<step-samples>1</step-samples>	</sampling-scale>	<ChannelInfo name="langmuir$rec.exp.langmuir.hardwareinfo.channel.0.name" independent="false" direction="CHANNEL_INPUT">		<scale label="langmuir$rec.exp.langmuir.hardwareinfo.channel.0.label"			physicsunitname="langmuir$rec.exp.langmuir.hardwareinfo.channel.0.physicsunitname"			physicsunitsymbol="V" multiplier="none" selected="true">			<min><PhysicsVal type="float">0</PhysicsVal></min>			<max><PhysicsVal type="float">10000</PhysicsVal></max>			<step><PhysicsVal type="float">1</PhysicsVal></step>			<errordefault><PhysicsVal type="float">1E-5</PhysicsVal></errordefault>		</scale>	</ChannelInfo>	<ChannelInfo name="langmuir$rec.exp.langmuir.hardwareinfo.channel.1.name" independent="false" direction="CHANNEL_INPUT">		<scale label="langmuir$rec.exp.langmuir.hardwareinfo.channel.1.label"			physicsunitname="langmuir$rec.exp.langmuir.hardwareinfo.channel.1.physicsunitname"			physicsunitsymbol="A" multiplier="micro" selected="true">			<min><PhysicsVal type="float">10</PhysicsVal></min>			<max><PhysicsVal type="float">100</PhysicsVal></max>			<step><PhysicsVal type="float">1</PhysicsVal></step>			<errordefault><PhysicsVal type="float">1E-5</PhysicsVal></errordefault>		</scale>	</ChannelInfo>	<ChannelInfo name="langmuir$rec.exp.langmuir.hardwareinfo.channel.2.name" independent="false" direction="CHANNEL_INPUT">		<scale label="langmuir$rec.exp.langmuir.hardwareinfo.channel.2.label"			physicsunitname="langmuir$rec.exp.langmuir.hardwareinfo.channel.2.physicsunitname"			physicsunitsymbol="Bar" multiplier="milli" selected="true">			<min><PhysicsVal type="float">0</PhysicsVal></min>			<max><PhysicsVal type="float">10000</PhysicsVal></max>			<step><PhysicsVal type="float">1</PhysicsVal></step>			<errordefault><PhysicsVal type="float">1E-5</PhysicsVal><