
<jnlp spec="1.0+" codebase="${jnlp.codebase}" href="launch.jnlp">
    <information>
        <title>${APPLICATION.TITLE}</title>
        <vendor>${APPLICATION.VENDOR}</vendor>
        <homepage href="${APPLICATION.HOMEPAGE}"/>
        <description>${APPLICATION.DESC}</description>
        <description kind="short">${APPLICATION.DESC.SHORT}</description>
<!--${JNLP.ICONS}-->
<!--${JNLP.OFFLINE.ALLOWED}-->
    </information>
<!--${JNLP.SECURITY}-->

    <resources>
<!--${JNLP.RESOURCES.RUNTIME}-->
<!--${JNLP.RESOURCES.MAIN.JAR}-->

<!--${JNLP.RESOURCES.JARS .DISABLED}-->
<!-- jnlp resources jars - auto-generate removing .DISABLED above -->
        <jar href="lib/appframework-1.0.3.jar"/>
        <jar href="lib/swing-worker-1.1.jar"/>
        <jar href="lib/swingx.jar"/>
        <jar href="lib/openorb_orb-1.4.0.jar"/>
        <jar href="lib/openorb_orb_omg-1.4.0.jar"/>
        <jar href="lib/openorb_ots-1.4.0.jar"/>
        <jar href="lib/openorb_pss-1.4.0.jar"/>
        <jar href="lib/avalon-framework.jar"/>
        <jar href="lib/comm.jar"/>
        <jar href="lib/fits1.3.jar"/>
        <jar href="lib/JCFN.jar"/>
        <jar href="lib/jcommon-1.0.12.jar"/>
        <jar href="lib/jfreechart-1.0.9.jar"/>
        <jar href="lib/jmf.jar"/>
		<jar href="lib/jna.jar"/>
        <jar href="lib/jsr80.jar"/>
        <jar href="lib/jsr80_ri.jar"/>
        <jar href="lib/logkit.jar"/>
        <jar href="lib/RXTXcomm.jar"/>
        <jar href="lib/tools-1.4.0.jar"/>
        <jar href="lib/xercesImpl.jar"/>
        <jar href="lib/xml-apis.jar"/>
        <jar href="lib/ReCClient.jar"/>
        <jar href="lib/ReCCommon.jar"/>
        <jar href="lib/ReCHardwareServer.jar"/>
        <jar href="lib/ReCMulticastController.jar"/>
        <jar href="lib/scubaClient.jar"/>
        <jar href="lib/radioactividadeClient.jar"/>
        <jar href="lib/conducaocalorClient.jar"/>
        <jar href="lib/gClient.jar"/>
        <jar href="lib/pvClient.jar"/>
        <jar href="lib/penduloClient.jar"/>

<!--${JNLP.RESOURCES.EXTENSIONS}-->
        <extension name="OpenORBClient" href="openORB/OpenORBClient.jnlp" />
        
        <property name="ReCFaceConfig" value="config/ReCFaceConfig.xml" />
        <property name="ReCBaseUIConfig" value="etc/ReCBaseUIConfig.xml" />
        <property name="org.omg.CORBA.ORBClass" value="org.openorb.orb.core.ORB" />
        <property name="org.omg.CORBA.ORBSingletonClass" value="org.openorb.orb.core.ORBSingleton" />
        <property name="ReC.MultiCastController.BindName" value="MultiCastController" />
        <property name="ReC.MultiCastController.InitRef" value="MultiCastController" />
        <property name="openorb.config" value="etc/OpenORB.xml" />
        <property name="openorb.profile" value="ReCClient" />
        <property name="vlc.plugins.destdir" value=".eLab/native/plugins"/>
        <property name="vlc.plugins.filename" value="vlcplugins.zip"/>
        
        <!-- change this value to disable video -->
        <property name="video.development.enabled" value="yes"/>
        
    </resources>

    <!-- *******************************************************************
         ****************** Resources por SO / Arch ************************
         ******************************************************************* -->

<!-- falta archs x86-64 (ou x86_64?), amd64 e amd-->
    <resources arch="x86" os="Windows">
        <nativelib href="lib/libsvlc_win32_x86.jar"/>
        <extension href="windowsInstaller.jnlp" name="windowsInstaller"/>
    </resources>
    
    <resources arch="x86_64" os="Windows">
        <nativelib href="lib/libsvlc_win32_x86.jar"/>
        <extension href="windowsInstaller.jnlp" name="windowsInstaller"/>
    </resources>
    
    <resources arch="amd" os="Windows">
        <nativelib href="lib/libsvlc_win32_x86.jar"/>
        <extension href="windowsInstaller.jnlp" name="windowsInstaller"/>
    </resources>

    <resources arch="amd64" os="Windows">
        <nativelib href="lib/libsvlc_win32_x86.jar"/>
        <extension href="windowsInstaller.jnlp" name="windowsInstaller"/>
    </resources>

<!-- falta i686 e i586(? isto existe?). também pode ser usado x86_64, x86, sparc, ppc, ppc64 e armv41 -->
