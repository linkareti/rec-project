        <extension href="linuxInstaller.jnlp" name="linuxInstaller"/>
    </resources>
    <resources arch="amd64" os="Linux">
        <nativelib href="lib/libsvlc_linux_amd64.jar"/>
        <extension href="linuxInstaller.jnlp" name="linuxInstaller"/>
    </resources>
