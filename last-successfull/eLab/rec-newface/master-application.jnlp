        <nativelib href="lib/libsvlc_linux_i686.jar"/>
        <extension href="linuxInstaller.jnlp" name="linuxInstaller"/>
    </resources>
    <resources arch="i586" os="Linux">
        <nativelib href="lib/libsvlc_linux_i686.jar"/>
