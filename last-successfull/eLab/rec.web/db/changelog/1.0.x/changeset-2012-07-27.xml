<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet id="2012-07-26-UPDATE" author="gpereira">
		<update tableName="LABORATORY">
			<column name="IS_ACTIVE" valueNumeric="1"></column>
		</update>
	</changeSet>
	<changeSet id="2012-07-27-add_badword" author="gpereira">
		<insert tableName="BAD_WORD">
			<column name="version" value="0" />
			<column name="regex" value="(fdp|caralho|porra|gay|puta)" />
		</insert>
	</changeSet>
</databaseChangeLog>