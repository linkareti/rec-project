<?xml version="1.0"?>
<!DOCTYPE jboss-web PUBLIC
   "-//JBoss//DTD Web Application 5.0//EN"
   "http://www.jboss.org/j2ee/dtd/jboss-web_5_0.dtd">
<jboss-web>
	<context-root>rec.web</context-root>
	<resource-ref>
		<res-ref-name>jdbc/allocation</res-ref-name>
		<jndi-name>java:/jdbc/allocation</jndi-name>
	</resource-ref>
	<resource-ref>
		<res-ref-name>com.linkare.rec.web.service.MailServiceBean/queue</res-ref-name>
		<jndi-name>java:/jms/MailMessages</jndi-name>
	</resource-ref>
	<resource-ref>
		<res-ref-name>com.linkare.rec.web.service.MailServiceBean/mailConnectionFactory</res-ref-name>
		<jndi-name>java:/ConnectionFactory</jndi-name>
	</resource-ref>
	<resource-ref>
		<res-ref-name>com.linkare.rec.web.service.MailServiceBean/mailSession</res-ref-name>
		<jndi-name>java:/mail/mailSession</jndi-name>
	</resource-ref>
</jboss-web>