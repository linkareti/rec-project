        <jar href="lib/vlcplugins_mac_ppc64.jar"/>
    </resources>
    <resources arch="x86" os="Mac OS X">
        <jar href="lib/vlcplugins_mac_ppc64.jar"/>
    </resources>
    <resources arch="x86_64" os="Mac OS X">
        <jar href="lib/vlcplugins_mac_ppc64.jar"/>
    </resources>-->

    <installer-desc main-class="com.linkare.rec.impl.newface.installer.mac.MacInstaller"/>
</jnlp>
