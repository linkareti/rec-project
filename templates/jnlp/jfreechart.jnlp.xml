<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE jnlp PUBLIC "-//Sun Microsystems, Inc//DTD JNLP Descriptor 6.0.10//EN" "http://java.sun.com/dtd/JNLP-6.0.10.dtd">
<jnlp spec="1.5+" codebase="@codebase@" href="jfreechart.jnlp">
	<information>
		<title>@jfreechart.title@</title>
		<vendor>@jfreechart.vendor@</vendor>
		<homepage href="@jfreechart.homepage@" />
		<description>@jfreechart.description@</description>
		<description kind="one-line">@jfreechart.description.oneline@</description>
		<description kind="tooltip">@jfreechart.description.tooltip@</description>
		<description kind="short">@jfreechart.description.short@</description>
		<offline-allowed />
	</information>
	<update check="always" policy="always" />
	<resources>
		<jar href="lib/jfreechart-1.0.9_sig.jar" />
		<jar href="lib/jcommon-1.0.12_sig.jar" />
	</resources>
	<component-desc />
	<security>
		<all-permissions />
	</security>
</jnlp>
