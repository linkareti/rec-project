<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<changeSet author="pzenida"
		id="2017-05-29-fixing-key-parish-on-naturality">
		<preConditions onFail="MARK_RAN">
			<columnExists tableName="naturality" columnName="key_parish" />
		</preConditions>
		<renameColumn tableName="naturality" oldColumnName="key_parish"
			newColumnName="key_parish" columnDataType="bigint(20)" />
	</changeSet>

	<changeSet author="pzenida" id="2017-05-29-add-fk-parish-on-naturality">
		<preConditions onFail="MARK_RAN">
			<not>
				<foreignKeyConstraintExists
					foreignKeyName="fk_parish_on_naturality" foreignKeyTableName="naturality" />
			</not>
		</preConditions>
		<addForeignKeyConstraint constraintName="fk_parish_on_naturality"
			referencedTableName="administrative_division" baseColumnNames="key_parish"
			baseTableName="naturality" referencedColumnNames="id" />
	</changeSet>

</databaseChangeLog>