		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="birth_registration"
					columnName="previous_birth_date" />
			</not>
		</preConditions>
		<addColumn tableName="birth_registration">
			<column name="previous_birth_date" type="date" />
		</addColumn>
	</changeSet>

	<changeSet author="sbogas" id="2017-07-01-change-birth-registration-9">
		<preConditions onFail="MARK_RAN">
			<not>
				<columnExists tableName="birth_registration"
					columnName="previous_live_birth_date" />
